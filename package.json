    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "chai": "=4.5.0",
    "chai-as-promised": "=7.1.2",
    "core-js": "^3.40.0",
    "dotenv": "^16.4.7",
    "express": "^4.19.2",
    "express-session": "^1.18.1",
    "jquery": "^3.7.1",
    "jwt-decode": "^4.0.0",
    "mocha": "^11.0.1",
    "multer": "^1.4.5-lts.1",
