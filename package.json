    "@types/chai": "^5.0.1",
    "@types/chai-as-promised": "^8.0.1",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.30",
    "@types/nunjucks": "^3.2.6",
